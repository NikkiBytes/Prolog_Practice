    <span style="font-weight: bold"><span style="color: #0000FF">format</span></span><span style="color: #990000">(</span><span style="color: #FF0000">'fib(~w)=~w~n'</span><span style="color: #990000">,</span><span style="color: #990000">[</span><span style="color: #009900">X</span><span style="color: #990000">,</span><span style="color: #009900">V</span><span style="color: #990000">]</span><span style="color: #990000">)</span><span style="color: #990000">.</span>
  
  </tt>
</pre>
